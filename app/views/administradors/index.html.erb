<head>
  <%= stylesheet_link_tag :administradors %>
</head>
<p id="notice"><%= notice %></p>

<h1 id="titulo">Administradores</h1>
<%= link_to 'New Administrador', new_administrador_path %><br>
<div class="contenedor_listado">
 <div id="listado-administradores">
    <% @administradors.each do |administrador| %>
       <div class="administrador">
          <div class="user_picture">
            <%= image_tag "woman.png", :size => "50x50"%>
          </div>
          <div class="nombre-administrador">
            <%= administrador.username %>
          </div>
          <div class="footer_administrador">
           <div class="btn_administrador">
            <%= link_to 'Edit', edit_administrador_path(administrador) %>
            <%= link_to 'Destroy', administrador, method: :delete, data: { confirm: 'Are you sure?' } %>
           </div>
          </div>
       </div>
    <% end %>
 </div>
</div>
